{
    "configurations": [
        {
            "type": "docker-compose-up",
            "name": "Dev UP",
            "files": [
                "docker-compose-dev.yml"
            ]
        },
        {
            "type": "spring-boot",
            "name": "Auth configuration",
            "mainClass": "me.tuhin47.auth.AuthService",
            "workingDir": "spring-boot-oauth2-social-login/",
            "module": "spring-boot-oauth2-social-login"
        },
        {
            "type": "command",
            "name": "Dev Stop",
            "program": "/usr/local/bin/docker-compose",
            "args": [
                "-f",
                "docker-compose-dev.yml",
                "stop"
            ]
        },
        {
            "type": "maven",
            "name": "common mvn",
            "workingDir": "common/",
            "tasks": ["clean","install"]
        }

    ]
}